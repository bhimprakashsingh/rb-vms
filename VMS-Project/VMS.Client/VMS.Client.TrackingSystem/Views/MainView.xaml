        	<Setter Property="Template">
        		<Setter.Value>
        			<ControlTemplate TargetType="{x:Type TreeViewItem}">
        				<Grid>
        					<Grid.ColumnDefinitions>
        						<ColumnDefinition MinWidth="19" Width="Auto"/>
        						<ColumnDefinition Width="Auto"/>
