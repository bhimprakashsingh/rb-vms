									LastChildFill="False"
									d:LayoutOverrides="GridBox"
									Grid.RowSpan="2"
									Margin="0,25,0,0">
									<DockPanel.LayoutTransform>
										<TransformGroup>
											<ScaleTransform ScaleX="1"
												ScaleY="1" />
											<SkewTransform AngleX="0"
												AngleY="0" />
											<RotateTransform Angle="-90" />
											<TranslateTransform X="0"
												Y="0" />
										</TransformGroup>
									</DockPanel.LayoutTransform>
									<DockPanel.RenderTransform>
										<TransformGroup>
											<ScaleTransform ScaleX="1"
												ScaleY="1" />
											<SkewTransform AngleX="0"
												AngleY="0" />
											<RotateTransform Angle="0" />
											<TranslateTransform X="0"
												Y="0" />
										</TransformGroup>
									</DockPanel.RenderTransform>
									<Button Style="{DynamicResource MyVMSNavigatorButtonStyle}"
										Content="VMS  Navigator"
										BorderThickness="0.5,0.5,0.5,0.5"
										FontSize="14"
										FontWeight="Bold"
										Foreground="#FF2B5597"
										RenderTransformOrigin="0.5,0.5"
										DockPanel.Dock="Top"
										Background="#FFDFECFF"
										BorderBrush="#FF2B5597"
										x:Name="myNavigationPaneButton"
										Visibility="Collapsed">
										<Button.RenderTransform>
											<TransformGroup>
												<ScaleTransform ScaleX="1"
													ScaleY="1" />
												<SkewTransform AngleX="0"
													AngleY="0" />
												<RotateTransform Angle="0" />
												<TranslateTransform X="0"
													Y="0" />
											</TransformGroup>
										</Button.RenderTransform>
									</Button>
								</DockPanel>
							</Grid>
						</Border>
						<GridSplitter HorizontalAlignment="Stretch"
							VerticalAlignment="Top"
							Height="5"
							d:LayoutOverrides="GridBox"
							Style="{DynamicResource GridSplitterStyle1}"
							Grid.Row="1" Background="#FFDFEDFF" />
						<Border
							Margin="0,5,0,0"
							VerticalAlignment="Stretch"
							Height="Auto"
							BorderBrush="#FF2B5597" x:Name="brdToday" Grid.Row="1" BorderThickness="1,1,1,1" d:LayoutOverrides="GridBox">
							<Grid x:Name="grdToday" Width="Auto" Height="Auto">
								<Grid.RowDefinitions>
									<RowDefinition Height="22"/>
									<RowDefinition Height="*"/>
								</Grid.RowDefinitions>
								<Grid x:Name="grdBgToday">
									<Rectangle HorizontalAlignment="Stretch" VerticalAlignment="Stretch" x:Name="bgToday">
										<Rectangle.Fill>
											<LinearGradientBrush
												EndPoint="0.5,1"
												StartPoint="0.5,0">
												<GradientStop Color="#FFCDE3FF" Offset="0"/>
												<GradientStop Color="#FFA9CEFF" Offset="1"/>
											</LinearGradientBrush>
										</Rectangle.Fill>
									</Rectangle>
									<Border x:Name="brdHight" SnapsToDevicePixels="True" BorderBrush="#FFFFFFFF" BorderThickness="1,1,0,0"/>
									<TextBlock FontSize="12"
										FontWeight="Bold"
										Foreground="#FF2B5597"
										Text="Thông tin"
										TextWrapping="NoWrap" HorizontalAlignment="Left" Margin="10,0,0,0" x:Name="txtblToday" VerticalAlignment="Center" d:LayoutOverrides="Height, GridBox" />
									<Rectangle Fill="#FF2B5597" StrokeThickness="0" x:Name="line" VerticalAlignment="Bottom" Width="Auto" Height="1"/>
								</Grid>
								<Grid x:Name="grdTodayContent" Grid.Row="1" Background="#FFFFFFFF" d:IsHidden="True" />
							</Grid>
						</Border>
					</Grid>
					<Grid x:Name="grdMainControl" Grid.Column="1" Margin="3,0,0,0">
                    </Grid>
				</Grid>
			</Border>
			
		</DockPanel>
	</Border>
</Window>
